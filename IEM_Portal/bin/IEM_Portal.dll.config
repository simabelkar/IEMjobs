<?xml version="1.0"?>
<configuration>

    <connectionStrings>
      <add name="IEMJobsConnectionString" connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\IEMJobs_DB.mdf;Integrated Security=True;Connect Timeout=30"
           providerName="System.Data.SqlClient" />
    </connectionStrings>
  
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>

    <system.webServer>
      <defaultDocument>
        <files>
          <clear/>
          <add value="homepage.aspx"/>
        </files>
      </defaultDocument>
    </system.webServer>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>
</configuration>
